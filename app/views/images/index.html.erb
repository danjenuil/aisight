<h1>Images</h1>

<table>
  <thead>
        <tr>
            <th>Image</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
      <% @images.each do |image| %>
        <tr>
          <td><%= image.title %></td>
          <td><%= link_to 'View', image_path(image) %></td>
        </tr>
      <% end %>
    </tbody>
</table>